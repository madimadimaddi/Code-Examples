      function jsondata(jsonfilecontent) {
          //console.log(jsonfilecontent);
        jsonString = JSON.parse(jsonfilecontent);//jsonfilecontent;
        //console.log(jsonString);
     
         var monthsKey = [
          "January",
          "February",
          "March",
          "April",
          "May",
          "June",
          "July",
          "August",
          "September",
          "October",
          "November",
          "December"         
         ];
         
     // Set counter
      var currentIndex = 0,
      itemAmt = jsonString.length,
      imagePath = "https://irp-cdn.multiscreensite.com/22b3e3c9/dms3rep/multi/",
      imageName = "350x375.jpg";
    
     for (var u = 1; u <= itemAmt; u++) { 
      $('div.container').append('<div class="data_container"><img class="keeperImage" src="https://irp-cdn.multiscreensite.com/22b3e3c9/dms3rep/multi/08-10-John-350x375.jpg"/><span class="keeper-wrap"><p class="keeper_info"><span class="keeper">Keeper</span><span class="k_name"></span></p><p class="location"><span class="location-title"> Location </span><span class="keeper_location"></span></p></span><span class="circle-graph"></span><p class="keeper_date"></p></div>');
      }
      var items = $('.container div.data_container');
         
         
      var starterCounter = itemAmt -1,
      workingIndex = starterCounter,
      workingIndex1 = itemAmt;
        var dataJson = jsonString, monthRaw, sliderMonth;
        var len = dataJson.length - 1, sliderPosition = 0;
        
        monthRaw = dataJson[len].started.substring(5,7), sliderMonth = parseInt(monthRaw);
        
            // Establish progress bar position
      if (sliderMonth >= 10) {
        sliderPosition = sliderMonth;
        
      } else if(sliderMonth == 9) {
        sliderPosition = sliderMonth;
        
      } else {
        sliderPosition = sliderMonth + 12;
      }
      
      sliderPosition = 20;
      $( "#slider" ).slider( "value", sliderPosition);
      
          
              
              function indexMover() {
               var length1 = dataJson.length, workingDate = selectedIndex;
               workingIndex = length1 + selectedIndex;
               
      // Get dates among other things        
              
              var started1 = dataJson[workingIndex].started.split(" "), fulldate = started1[0], currentdate, currentyear, 
          fullDateString, 
          st = dataJson[workingIndex].started,
          st_one = dataJson[workingIndex-1].started,
          st_two = dataJson[workingIndex-2].started;
          //st_three = dataJson[workingIndex-3].started;
          
          
          // Get a string of dates up to 4 days ago
          var fullDateString = st.replace(/[^A-Z0-9]/ig, "-"), string_one = st_one.replace(/[^A-Z0-9]/ig, "-"), string_two = st_two.replace(/[^A-Z0-9]/ig, "-");/*, string_three = st_three.replace(/[^A-Z0-9]/ig, "-");*/
          
          // Get year month day of the last 3 days 
          /*var th = string_three.substring(0,10),*/var tw = string_two.substring(0,10), one = string_one.substring(0,10), zero = st.substring(0,10);
        
          // get month as single digit for current day
        var parts = fulldate.split("-"), mprefix = parts[1].substring(0,1), month;
        
           // get month as single digit for the last 3 days
           
         /* var previousDates3 = th.split("-"), mprefix3 = previousDates3[1].substring(0,1), month3,*/  
         var previousDates2 = tw.split("-"), mprefix2 = previousDates2[1].substring(0,1), month2,
         previousDates1 = one.split("-"), mprefix1 = previousDates1[1].substring(0,1), month1;
      
        // Get parts 
        
        /*var the_date3 = previousDates3[2],*/
           var the_date2 = previousDates2[2],
          the_date1 = previousDates1[2],
           /* the_year3 = previousDates3[0],*/
          the_year2 = previousDates2[0],
          the_year1 = previousDates1[0];
          
  /*var currentdate3 = previousDates3[2],*/
    var   currentdate2 = previousDates2[2],
    currentdate1 = previousDates1[2];
    
  /*var currentYear3 = previousDates3[0],*/
    var currentYear2 = previousDates2[0],
    currentYear1 = previousDates1[0];
          
    //if (mprefix3==0){ month3=previousDates3[1].substring(1,2);}else{month3=month3=previousDates3[1];}
    
    if (mprefix2==0){ month2=previousDates2[1].substring(1,2);}else{month2=month2=previousDates2[1];}
    
    if (mprefix1==0){ month1=previousDates1[1].substring(1,2);}else{month1=month1=previousDates1[1];}
    
    //test 
    //console.log(currentdate2 + ' ' + monthsKey[month2-1] + ' ' + currentYear2);
        
        
        currentdate = parts[2];
        currentyear = parts[0];
          if (mprefix == 0){
            month = parts[1].substring(1,2);
                //console.log(month)
          } else {
            month = parts[1];
          }
      var todayFullDate = currentdate + " " + monthsKey[month-1] + " " + currentyear;
              
              
              var counter = $('div.name-content').length;
              //if (counter == 0) {
            
            //console.log(imagePath + month3 + "-"+ currentdate3 +'-'+dataJson[workingIndex-3].name/*.toLowerCase()*/+'-'+imageName);
            
         // Get images
          
          //$('.container div.data_container img.keeperImage').eq(workingIndex-3).attr('src',imagePath + th.substring(5) +'-'+dataJson[workingIndex-3].name/*.toLowerCase()*/+'-'+imageName).attr('onerror','this.src="https://irp-cdn.multiscreensite.com/22b3e3c9/dms3rep/multi/placeholder4-350x375.jpg";');
           
        $('.container div.data_container img.keeperImage').eq(workingIndex-2).attr({src:imagePath + tw.substring(5) +'-'+dataJson[workingIndex-2].name/*.toLowerCase()*/+'-'+imageName, onerror: 'this.src="https://irp-cdn.multiscreensite.com/22b3e3c9/dms3rep/multi/placeholder3-350x375.jpg";'});
        
        $('.container div.data_container img.keeperImage').eq(workingIndex-1).attr({src:imagePath + one.substring(5) +'-'+dataJson[workingIndex-1].name/*.toLowerCase()*/+'-'+imageName, onerror: 'this.src="https://irp-cdn.multiscreensite.com/22b3e3c9/dms3rep/multi/placeholder2-350x375.jpg";'});
        
        $('.container div.data_container.data_container img.keeperImage').eq(workingIndex).attr({src:imagePath + zero.substring(5) +'-'+dataJson[workingIndex].name/*.toLowerCase()*/+'-'+imageName, onerror: 'this.src="https://irp-cdn.multiscreensite.com/22b3e3c9/dms3rep/multi/placeholder1-350x375.jpg";'});
        
        // Get names
          
        //$('.container div.data_container p.keeper_info .k_name').eq(workingIndex-3).text(dataJson[workingIndex-3].name);  
