       });
    
      
    function cycleItems() {
      
        var item = $('.container div.data_container').eq(selectedIndex);
       
          var prevItem1 = $('.container div.data_container').eq(currentIndex - 1);
          var prevItem2 = $('.container div.data_container').eq(currentIndex - 2);
          var prevItem3 = $('.container div.data_container').eq(currentIndex - 3);
         //var prevItem4 = $('.container div.data_container').eq(currentIndex - 4);
        //items.hide();
        items.addClass('show_none');
        items.removeClass('currentImg');
        items.removeClass('prevDisplay prevDisplay1 prevDisplay2 prevDisplay3');
        //item.css('display','inline-block');
          item.addClass('currentImg');
        prevItem1.addClass('prevDisplay');
        prevItem2.addClass('prevDisplay prevDisplay1');
        prevItem3.addClass('prevDisplay prevDisplay2');
        //prevItem4.addClass('prevDisplay prevDisplay3');
        
        prevItem1.removeClass('show_none');
        prevItem2.removeClass('show_none');
        prevItem3.removeClass('show_none');
        //prevItem4.removeClass('show_none');
    }
    
  
    $('.next').click(function() {
      /*var selection = $( "#slider" ).slider( "value");
      $( "#slider" ).slider( "value", selection + 1);*/
        selectedIndex += 1;
      currentIndex += 1;
      $('.prev').removeAttr('disabled','disabled');
      
        if (currentIndex > itemAmt - 1) {
        currentIndex = 0;
        }
        
        if (currentIndex == 0) {
          $(this).attr('disabled','disabled');
         
          
        } 
        indexMover();
        
      cycleItems();
    });
    
    
    $('.prev').click(function() {
      /*var selection = $( "#slider" ).slider( "value");
      $( "#slider" ).slider( "value", selection - 1);*/
        //console.log($('.currentImg').index());
    if ($('.currentImg').index() == 3) {
        $(this).attr('disabled','disabled');
      }
        selectedIndex -= 1;
        currentIndex -= 1;
        if (currentIndex < 0) {
        currentIndex = itemAmt - 1;
        }
       
        $('.next').removeAttr('disabled','disabled');
        if (currentIndex == itemAmt - 1) {
           //$( "#slider" ).slider( "value", currentIndex );
        }
        
        if (currentIndex == 0) {
          $(this).attr('disabled','disabled');
        //  console.log("we are at the end");
        } 
        
        
        
        indexMover();
        cycleItems();
      });
      
      };
     });
     
     
     
});
  
  
    </script>
  <script>
  // Find all iframes
var $iframes = $( "iframe" );
 
// Find &#x26; save the aspect ratio for all iframes
$iframes.each(function () {
  $( this ).data( "ratio", this.height / this.width )
    // Remove the hardcoded width &#x26; height attributes
    .removeAttr( "width" )
    .removeAttr( "height" );
});
 
// Resize the iframes when the window is resized
$( window ).resize( function () {
  $iframes.each( function() {
    // Get the parent container&#x27;s width
    var width = $( this ).parent().width();
    $( this ).width( width )
      .height( width * $( this ).data( "ratio" ) );
  });
// Resize to fix all iframes on page load.
}).resize();
  
 
  
  </script>

